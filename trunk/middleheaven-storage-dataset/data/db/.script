CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE SEQUENCE SEQ_TBPESSOA AS INTEGER START WITH 0 
CREATE SEQUENCE SEQ_TBRENDA AS INTEGER START WITH 0 
CREATE SEQUENCE SEQ_TBVEICULO AS INTEGER START WITH 0 
CREATE CACHED TABLE TBVEICULO(MODELO BIGINT NOT NULL,VALOR NUMERIC(12,12) NOT NULL,ID BIGINT NOT NULL PRIMARY KEY,ANO BIGINT NOT NULL,IDPESSOA BIGINT NOT NULL)
CREATE CACHED TABLE TBPESSOA(NOME VARCHAR(150) NOT NULL,ID BIGINT NOT NULL PRIMARY KEY,ANIVERSARIO TIMESTAMP NOT NULL)
CREATE CACHED TABLE TBRENDA(DATA TIMESTAMP NOT NULL,RENDA NUMERIC(12,12) NOT NULL,ID BIGINT NOT NULL PRIMARY KEY,IDPESSOA BIGINT NOT NULL)
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
